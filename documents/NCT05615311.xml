    <arm_group_type>Placebo Comparator</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Dietary Supplement</intervention_type>
    <intervention_name>Vitamin D supplementation</intervention_name>
    <description>Study participants assigned to the intervention group will receive 800 IU/ day of vitamin D plus 200-300 IU/day of vitamin D from parenteral and enteral sources during the first 2 weeks after birth.</description>
    <arm_group_label>Vitamin D group</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>No additional vitamin D supplementation</intervention_name>
    <description>Study participants assigned to the control group will receive 200-300 IU/ day of vitamin D from parenteral and enteral sources during the first 2 weeks after birth.</description>
    <arm_group_label>Control group</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Gestational age of 28 weeks or less&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Major congenital anomalies and infants&#xD;
&#xD;
          -  Terminal illness in whom decisions to withhold or limit life support have been made&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>1 Day</minimum_age>
    <maximum_age>4 Days</maximum_age>
