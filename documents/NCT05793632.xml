  <brief_title>Quetiapine in Prevention of Delirium</brief_title>
  <official_title>Role of Quetiapine in the Prevention of ICU Delirium in Elderly Patients at a High Risk</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ain Shams University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
